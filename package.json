{
  "name": "@j-mcgregor/bun-express",
  "repository": "https://github.com/j-mcgregor/bun-express.git",
  "module": "index.ts",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "version": "0.0.6",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "dev": "bun run --watch ./src/server.tsx",
    "build": "bun build src/app.ts --target=node --outfile=dist/index.js; cp ./types/index.d.ts dist/index.d.ts"
  },
  "type": "module",
  "dependencies": {
    "path-to-regexp": "^7.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "publishConfig": {
    "registry": "https://github.com/j-mcgregor/bun-express.git"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./src/app.ts"
    },
    "./types": "./dist/index.d.ts"
  }
}
